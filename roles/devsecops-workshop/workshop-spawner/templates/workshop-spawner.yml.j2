      app: devsecops-workshop-dashboard
      class: spawner
      spawner: devsecops-workshop-dashboard
    name: devsecops-workshop-dashboard-spawner-data
    namespace: {{ workshop_spawner_project_name }}
  spec:
    accessModes:
    - ReadWriteOnce
    resources:
      requests:
        storage: 1Gi
- apiVersion: apps.openshift.io/v1
  kind: DeploymentConfig
  metadata:
    labels:
      app: devsecops-workshop-dashboard
      class: spawner
      spawner: devsecops-workshop-dashboard
    name: devsecops-workshop-dashboard-spawner
    namespace: {{ workshop_spawner_project_name }}
  spec:
    replicas: 1
    selector:
      deploymentconfig: devsecops-workshop-dashboard-spawner
    strategy:
      type: Recreate
    template:
      metadata:
        labels:
          app: devsecops-workshop-dashboard
          class: spawner
          deploymentconfig: devsecops-workshop-dashboard-spawner
          spawner: devsecops-workshop-dashboard
      spec:
        containers:
        - env:
          - name: CONFIGURATION_TYPE
            value: external-keycloak
          - name: APPLICATION_NAME
            value: devsecops-workshop-dashboard
          - name: WORKSHOP_NAME
            value: devsecops-workshop-dashboard
          - name: HOMEROOM_NAME
            value: ""
          - name: WORKSHOP_IMAGE
            value: {{ workshop_dashboard_image }}
          - name: WORKSHOP_MEMORY
            value: 512Mi
          - name: CONSOLE_MEMORY
            value: 128Mi
          - name: CONSOLE_BRANDING
            value: openshift
          - name: CONSOLE_IMAGE
            value: quay.io/openshift/origin-console:{{ openshift_version|default("4.4") }}
          - name: VOLUME_SIZE
            value: ""
          - name: ADMIN_USERS
            value: ""
          - name: IDLE_TIMEOUT
            value: "7200"
          - name: RESOURCE_BUDGET
            value: default
          - name: DOWNLOAD_URL
            value: ""
          - name: WORKSHOP_FILE
            value: ""
          - name: OC_VERSION
            value: ""
          - name: ODO_VERSION
            value: ""
          - name: KUBECTL_VERSION
            value: ""
          - name: EXPOSED_PORTS
            value: ""
          - name: HOMEROOM_LINK
            value: ""
          - name: OAUTH_CLIENT_SECRET
            value: 8e135d2ae856dadee4eb02ee005e4e78
          - name: KEYCLOAK_AUTH_URL
            value: {{ keycloak_url }}/auth
          - name: KEYCLOAK_REALM
            value: codeready
          - name: KEYCLOAK_CLIENT_ID
            value: codeready-public
          - name: CUSTOM_TAB_1
            value: "CodeReady Workspaces=https://codeready-{{ crw_project_name }}.{{ cluster_router }}"
{% if deploy_rocketchat | bool %}
          - name: CUSTOM_TAB_2
            value: "Chat=https://rocketchat-{{ rocketchat_project_name }}.{{ cluster_router }}"
{% endif %}
          - name: API_SERVER
            value: https://api.{{ cluster_name }}.{{ openshift_base_domain }}:6443
          livenessProbe:
            failureThreshold: 3
            httpGet:
              path: /hub/api
              port: 8080
              scheme: HTTP
            periodSeconds: 10
            successThreshold: 1
            timeoutSeconds: 1
          name: spawner
          ports:
          - containerPort: 8080
            protocol: TCP
          readinessProbe:
            failureThreshold: 3
            httpGet:
              path: /hub/api
              port: 8080
              scheme: HTTP
            periodSeconds: 10
            successThreshold: 1
            timeoutSeconds: 1
          resources:
            limits:
              memory: 768Mi
            requests:
              memory: 768Mi
          volumeMounts:
          - mountPath: /opt/app-root/data
            name: data
          - mountPath: /opt/app-root/configs
            name: configs
          - mountPath: /opt/app-root/resources
            name: resources
        serviceAccountName: devsecops-workshop-dashboard-spawner
        volumes:
        - name: data
          persistentVolumeClaim:
            claimName: devsecops-workshop-dashboard-spawner-data
        - configMap:
            defaultMode: 420
            name: devsecops-workshop-dashboard-spawner-configs
          name: configs
        - configMap:
            defaultMode: 420
            name: devsecops-workshop-dashboard-session-resources
          name: resources
    triggers:
    - type: ConfigChange
    - imageChangeParams:
        automatic: true
        containerNames:
        - spawner
        from:
          kind: ImageStreamTag
          name: devsecops-workshop-dashboard-spawner:latest
      type: ImageChange
- apiVersion: v1
  kind: Service
  metadata:
    labels:
      app: devsecops-workshop-dashboard
      class: spawner
      spawner: devsecops-workshop-dashboard
    name: devsecops-workshop-dashboard-spawner
    namespace: {{ workshop_spawner_project_name }}
  spec:
    ports:
    - name: 8080-tcp
      port: 8080
      protocol: TCP
      targetPort: 8080
    - name: 8081-tcp
      port: 8081
      protocol: TCP
      targetPort: 8081
    selector:
      deploymentconfig: devsecops-workshop-dashboard-spawner
- apiVersion: route.openshift.io/v1
  kind: Route
  metadata:
    annotations:
      homeroom/description: ""
      homeroom/group: ""
      homeroom/title: ""
      kubernetes.io/tls-acme: "false"
    labels:
      app: devsecops-workshop-dashboard
      class: spawner
      spawner: devsecops-workshop-dashboard
    name: devsecops-workshop-dashboard-spawner
    namespace: {{ workshop_spawner_project_name }}
  spec:
    host: dashboard.{{ cluster_router }}
    port:
      targetPort: 8080-tcp
    tls:
      insecureEdgeTerminationPolicy: Redirect
      termination: edge
    to:
      kind: Service
      name: devsecops-workshop-dashboard-spawner
      weight: 100
- apiVersion: image.openshift.io/v1
  kind: ImageStream
  metadata:
    labels:
      app: devsecops-workshop-dashboard
      class: spawner
      spawner: devsecops-workshop-dashboard
    name: devsecops-workshop-dashboard-session
    namespace: {{ workshop_spawner_project_name }}
  spec:
    lookupPolicy:
      local: true
    tags:
    - from:
        kind: DockerImage
        name: {{ workshop_dashboard_image }}
      name: latest
kind: List
metadata: {}
